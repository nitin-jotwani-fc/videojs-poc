    "object-hash": "^2.0.3",
    "path-to-regexp": "6.2.0",
    "query-string": "^6.12.1",
    "raf-throttle": "^2.0.3",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.29.0",
    "react-lazyload": "^3.1.0",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.1.2",
    "react-twitter-embed": "^3.0.3",
    "styled-components": "^4.4.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
