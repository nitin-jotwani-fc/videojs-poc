{
  "name": "fc-web",
  "version": "3.2.16",
  "private": true,
  "description": "Web application for FanCode",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "npm run build:dev:server && npm run server",
    "server": "node server-build/desktop/main.js & node server-build/mobile/main.js & node server/main.js",
    "server:mobile": "node server-build/mobile/main.js & node server/main.js",
    "server:desktop": "node server-build/desktop/main.js & node server/main.js",
    "start:mobile": "npm run build:dev:server:mobile && npm run server:mobile",
    "start:desktop": "npm run build:dev:server:desktop && npm run server:desktop",
    "start:debug": "npm run build:dev:server && node --inspect server-build/main.js",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true npm run server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "proxy": "node server/main.js",
    "gql:update": "src/scripts/update-schema.js",
    "gql:compile": "src/scripts/gql-2-ts.js src",
    "gql": "yarn gql:update && yarn gql:compile",
    "lint": "npm run tsfiles -s | xargs tslint",
    "prettier": "src/scripts/prettier/prettier.sh",
    "prettier:diff": "git diff --name-only | grep '\\.ts\\|\\.tsx$' | xargs prettier --print-width 80 --single-quote --no-semi --no-bracket-spacing --write",
    "tsfiles": "git ls-files | grep '\\.ts\\|\\.tsx$'",
    "lint:fix": "npm run tsfiles -s | xargs tslint --fix",
    "test": "jest",
    "node-ts": "node-config-ts",
    "postinstall": "node-config-ts && yarn gql:compile && yarn tsc",
    "build:web": "NODE_ENV=staging webpack --config internals/webpack/webpack.prod.babel.js --color -p --hide-modules --display-optimization-bailout",
    "build:stag": "npm run build:stag:server && NODE_ENV=staging webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:prod": "npm run build:prod:server && NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:uat": "npm run build:uat:server && NODE_ENV=uat webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:fcload": "npm run build:load:server && NODE_ENV=load webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "create-component": "src/scripts/create-component/create-component.sh",
    "build:dev:server:mobile": "NODE_ENV=development PLATFORM=mobile webpack --config internals/webpack/server/webpack.dev.server.babel.js --mode=development",
    "build:dev:server:desktop": "NODE_ENV=development PLATFORM=desktop webpack --config internals/webpack/server/webpack.dev.server.babel.js --mode=development",
    "build:dev:server": "NODE_ENV=development webpack --config internals/webpack/server/webpack.dev.server.babel.js --mode=development",
    "build:stag:server": "NODE_ENV=staging webpack --config internals/webpack/server/webpack.prod.server.babel.js --mode=production",
    "build:load:server": "NODE_ENV=load webpack --config internals/webpack/server/webpack.prod.server.babel.js --mode=production",
    "build:prod:server": "NODE_ENV=production webpack --config internals/webpack/server/webpack.prod.server.babel.js --mode=production",
    "build:uat:server": "NODE_ENV=uat webpack --config internals/webpack/server/webpack.dev.server.babel.js --mode=development",
    "watch": "yarn tsc -w",
    "test:ci": "jest --coverage && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@action-land/core": "^8.0.0",
    "@action-land/match": "^8.0.0",
    "@action-land/smitten": "^8.0.0",
    "@action-land/tarz": "^8.0.0",
    "@brightcove/react-player-loader": "^1.3.0",
    "@loadable/component": "5.14.1",
    "@loadable/server": "5.14.0",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@types/animejs": "^3.1.0",
    "@types/facebook-js-sdk": "^3.3.0",
    "@types/react-helmet": "^6.0.0",
    "@types/react-lazyload": "^3.0.0",
    "@types/react-lottie": "^1.2.5",
    "animejs": "^3.1.0",
    "axios": "^0.18.0",
    "chalk": "^3.0.0",
