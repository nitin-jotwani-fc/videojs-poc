    "@loadable/babel-plugin": "5.13.2",
    "@loadable/webpack-plugin": "5.14.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.5",
    "@types/crypto-js": "^3.1.47",
    "@types/fontfaceobserver": "^0.0.6",
