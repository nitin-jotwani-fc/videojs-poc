    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "fluture": "^8.0.2",
    "fontfaceobserver": "^2.1.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.11.0",
    "helmet": "^4.0.0",
    "html-react-parser": "^0.13.0",
